@{
    ViewBag.Title = "Queue";
}

<div class="container text-center">
    <h3>Game <small>Queue</small></h3>
    <img width="30%" class="img-responsive center-block img-rounded" src="~/Content/pictures/logo.jpg" alt="wubba lubba dub zork">
    <div class="row text-center">
        <div class="loading">
            <svg width="300px" height="150px" viewBox="0 0 187.3 93.7" preserveAspectRatio="xMidYMid meet">
                <path id="infinity-outline" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10"
                      d="M93.9,46.4c9.3,9.5,13.8,17.9,23.5,17.9s17.5-7.8,17.5-17.5s-7.8-17.6-17.5-17.5c-9.7,0.1-13.3,7.2-22.1,17.1                c-8.9,8.8-15.7,17.9-25.4,17.9s-17.5-7.8-17.5-17.5s7.8-17.5,17.5-17.5S86.2,38.6,93.9,46.4z" />
                <path id="infinity-bg" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10"
                      d="M93.9,46.4c9.3,9.5,13.8,17.9,23.5,17.9s17.5-7.8,17.5-17.5s-7.8-17.6-17.5-17.5c-9.7,0.1-13.3,7.2-22.1,17.1                c-8.9,8.8-15.7,17.9-25.4,17.9s-17.5-7.8-17.5-17.5s7.8-17.5,17.5-17.5S86.2,38.6,93.9,46.4z" />
            </svg>
        </div>
        <br>
        <button id="button" type="submit" class="btn">Cancel</button>
    </div>
</div>

@section Scripts{
    @Scripts.Render("~/Scripts/jquery.signalR-2.2.2.min.js")
    @Scripts.Render("~/signalr/hubs")
    @Scripts.Render("~/Content/Javascripts/QueueHub.js")
    <script>
        $(document).ready(function () {
            $("#button").click(function () {
                window.location = "Index";
            })
        })
    </script>
}

